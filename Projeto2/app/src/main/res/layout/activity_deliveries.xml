<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Deliveries">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="500dp"
        android:layout_height="90dp"
        android:background="@android:color/darker_gray"
        android:gravity="center_vertical"
        android:text="  CambÃ£o"
        android:textSize="50sp"
        tools:layout_editor_absoluteX="-10dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="452dp"
        android:layout_height="96dp"
        android:background="#3BDCD6D6"
        android:gravity="center_vertical"
        android:text="  "
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-9dp" />

    <ImageButton
        android:id="@+id/imageButtonProf2"
        android:layout_width="89dp"
        android:layout_height="87dp"
        android:layout_marginStart="332dp"
        android:adjustViewBounds="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:srcCompat="@drawable/profile"
        tools:layout_editor_absoluteY="3dp"
        android:scaleType="fitXY"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="179dp"
        android:layout_height="55dp"
        android:layout_marginEnd="232dp"
        android:layout_marginBottom="412dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Meus Fretes"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/caixa"
        android:layout_width="145dp"
        android:layout_height="96dp"
        android:layout_marginStart="152dp"
        android:adjustViewBounds="true"
        android:backgroundTint="#F6F2F2"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/caminhao"
        app:srcCompat="@drawable/fretes"

        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/caminhao"
        android:layout_width="128dp"
        android:layout_height="95dp"
        android:backgroundTint="#F4F1F1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/caminhoes" />

    <ImageButton
        android:id="@+id/contato"
        android:layout_width="118dp"
        android:layout_height="91dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#FDFAFA"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/contato2" />

</androidx.constraintlayout.widget.ConstraintLayout>