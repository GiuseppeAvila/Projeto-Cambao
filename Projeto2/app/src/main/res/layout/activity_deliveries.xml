<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Deliveries">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="452dp"
        android:layout_height="96dp"
        android:background="#3BDCD6D6"
        android:gravity="center_vertical"
        android:text="  "
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-9dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="179dp"
        android:layout_height="55dp"
        android:layout_marginEnd="216dp"
        android:layout_marginBottom="444dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Recomendados"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/caixa"
        android:layout_width="145dp"
        android:layout_height="96dp"
        android:layout_marginStart="152dp"
        android:adjustViewBounds="true"
        android:backgroundTint="#F6F2F2"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/caminhao"
        app:srcCompat="@drawable/fretes"

        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/caminhao"
        android:layout_width="128dp"
        android:layout_height="95dp"
        android:backgroundTint="#F4F1F1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/caminhoes" />

    <ImageButton
        android:id="@+id/caminhao"
        android:layout_width="118dp"
        android:layout_height="91dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#FDFAFA"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/contato2" />

    <ScrollView
        android:layout_width="419dp"
        android:layout_height="335dp"
        tools:layout_editor_absoluteX="-9dp"
        tools:layout_editor_absoluteY="299dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>